#include<stdio.h>
void main ()
{
    int a,b;
    printf("Enter two numbers");
    scanf("%d%d", &a,&b);
    char op;
    printf("\n Enter symbols");
    scanf("%c",&op);
    switch (op)
    {
        case '+': 
            printf("the addition is %d",a+b);
            break;
        case '-':
            printf("the subtraction is %d",a-b);
            break;
        case '/': 
            printf("the division is %d",a/b);
            break;
        case '*': 
            printf("the multiplication %d",a*b);
            break;
    }
}